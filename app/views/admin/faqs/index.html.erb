<div id="site-content">
  <% @faqs.each do |faq| %>
      <div class="row">
        <div class="columns small-12 medium-4 large-4"><%= link_to faq.question, edit_admin_faq_path(faq) %> </div>
        <div class="columns small-12 medium-6 large-6"><%= faq.answer %></div>
        <div class="columns small-12 medium-1 large-1"><%= faq.visible %></div>
      </div>
  <% end %>
</div>
